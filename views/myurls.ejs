<!DOCTYPE html>
<html lang="en">
  <%- include('components/head') %>
  <body>
    <section id="layout">
      <%- include('components/navbar') %>
      <main id="auth-page">
        <div id="intro-section">
          <h1>My URLs</h1>
          <h3><% if(user){ %> Welcome <%= user.username %><% } %></h3>
        </div>
        <section class="border-space">
          <div id="url-table">
            <% if(jDataHTML.length===0){ %> <%- jDataHTML %> <% } else {%>
            <span id="please-add"
              >Please shorten some URLS to automatically add them to this
              table.</span
            >
            <% }%>
          </div>
        </section>
        <section id="result-space"></section>
      </main>
      <%- include('components/footer') %>
    </section>
    <script src="script.js"></script>
  </body>
</html>
